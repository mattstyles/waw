{
  "name": "@waw/components",
  "version": "0.0.0",
  "description": "Components for talos",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/mattstyles/waw"
  },
  "keywords": [
    "talos",
    "game",
    "components",
    "styled",
    "styled-components"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.1",
    "@storybook/react": "^3.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-styled-components": "^1.4.0",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-urban": "^1.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "open-color": "^1.6.2",
    "prop-types": "^15.6.0",
    "shevyjs": "^1.1.0",
    "styled-components": "^2.4.0"
