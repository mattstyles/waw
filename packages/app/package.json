{
  "name": "wizardry-and-witchcraft",
  "private": true,
  "version": "0.0.0",
  "description": "Wizardry and Witchcraft",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "NODE_PATH=src NODE_ENV=production parcel build src/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "NODE_PATH=src DEBUG=true parcel src/index.html"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-react-require",
      "transform-react-jsx",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ],
      "lodash",
      "transform-object-rest-spread"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/waw.git"
  },
  "keywords": [
    "raid",
    "preact"
  ],
  "author": "mattstyles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstyles/waw/issues"
  },
  "homepage": "https://mattstyles.github.io/waw",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-urban": "^1.0.1",
    "create-raid-app": "2.10.0",
    "gh-pages": "^1.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "parcel-bundler": "^1.3.1",
    "rimraf": "^2.6.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "@mattstyles/match": "^1.0.1",
    "@waw/components": "*",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "open-color": "^1.6.2",
    "prop-types": "^15.5.10",
    "raid": "^2.10.0",
    "raid-addons": "^2.10.0",
    "raid-fl": "^2.10.0",
    "raid-navigator": "^2.10.0",
    "raid-streams": "^2.10.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.0",
    "reselect": "^2.5.4",
    "simple-component-router": "0.0.2",
    "styled-components": "^2.4.0"
  }
}
